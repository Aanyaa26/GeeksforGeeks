19.07.24

class Solution{
    static long countWays(int N){
        int count = 0;
        for(int i=1; i<N;i++){
            for(int j=i;j<N;j++){
                for(int k=j;k<N;k++){
                    for(int l=k;l<N;l++){
                        if(i+j+k+l==N)
                        count ++;
                    }
                }
            }
            
        }
        return count;
    }
}
